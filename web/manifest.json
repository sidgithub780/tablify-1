{
  "manifest_version": 3,
  "name": "Tablify",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_popup": "tablify.html",
    "default_icon": "testImage.png"
  },
  "background": {
    "service_worker": "background.js"
  },

  "permissions": ["scripting", "tabs", "activeTab"],

  "host_permissions": ["*://*/*"]
}
